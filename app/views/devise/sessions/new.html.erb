<%= render 'devise/shared/sign_in_providers_list' %>

<h2><%= t('model.messages.sign_in_local_title') %></h2>
<%= simple_form_for(resource, :as => resource_name, :url => session_path(resource_name), :html => {:class => 'form-vertical'}) do |f| %>
    <%= render 'devise/shared/login_form_fields', f: f, identifier: :email %>
    <%= f.button :submit, "Sign in", :class => 'btn-primary' %>
<% end %>
<%= render "devise/shared/links" %>

